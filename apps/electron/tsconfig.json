{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "./main/**/*.ts", "./src/**/*.ts", "./src/**/*.tsx", "../../packages/utils/src/runPython.ts", "../../packages/utils/**/*.tsx", "../../packages/utils/global.d.ts"],
  "exclude": ["node_modules"]
}
